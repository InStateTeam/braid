.services-list
  background #000
  color rgba(255, 255, 255, 0.9)
  display inline-block
  float left
  font-size 20px
  margin 0
  min-height 1px
  overflow-y auto
  width 250px 
  z-index 100

  ul
    margin 0
    padding 0
    max-height calc(100vh - 115px)
    overflow-y auto
    text-align center
    
    li
      border-bottom 2px solid rgba(255, 255, 255, 0.9)
      list-style none
      padding 7px 10px
      cursor pointer